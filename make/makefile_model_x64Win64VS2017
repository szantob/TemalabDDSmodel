JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./AbstractTypes/DataModel/LocationType.java $(SOURCE_DIR)./AbstractTypes/DataModel/LocationTypeSeq.java $(SOURCE_DIR)./AbstractTypes/DataModel/LocationTypeTypeSupport.java $(SOURCE_DIR)./AbstractTypes/DataModel/LocationTypeTypeCode.java $(SOURCE_DIR)./AbstractTypes/DataModel/LocationTypeDataReader.java $(SOURCE_DIR)./AbstractTypes/DataModel/LocationTypeDataWriter.java $(SOURCE_DIR)./AbstractTypes/DataModel/SensorMetaType.java $(SOURCE_DIR)./AbstractTypes/DataModel/SensorMetaTypeSeq.java $(SOURCE_DIR)./AbstractTypes/DataModel/SensorMetaTypeTypeSupport.java $(SOURCE_DIR)./AbstractTypes/DataModel/SensorMetaTypeTypeCode.java $(SOURCE_DIR)./AbstractTypes/DataModel/SensorMetaTypeDataReader.java $(SOURCE_DIR)./AbstractTypes/DataModel/SensorMetaTypeDataWriter.java $(SOURCE_DIR)./AbstractTypes/DataModel/Data.java $(SOURCE_DIR)./AbstractTypes/DataModel/DataSeq.java $(SOURCE_DIR)./AbstractTypes/DataModel/DataTypeSupport.java $(SOURCE_DIR)./AbstractTypes/DataModel/DataTypeCode.java $(SOURCE_DIR)./AbstractTypes/DataModel/DataDataReader.java $(SOURCE_DIR)./AbstractTypes/DataModel/DataDataWriter.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherLocation.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherLocationSeq.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherLocationTypeSupport.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherLocationTypeCode.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherLocationDataReader.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherLocationDataWriter.java $(SOURCE_DIR)./AbstractTypes/Weather/Weather.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherSeq.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherTypeSupport.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherTypeCode.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherDataReader.java $(SOURCE_DIR)./AbstractTypes/Weather/WeatherDataWriter.java $(SOURCE_DIR)./MessageTypes/Weather/Temperature.java $(SOURCE_DIR)./MessageTypes/Weather/TemperatureSeq.java $(SOURCE_DIR)./MessageTypes/