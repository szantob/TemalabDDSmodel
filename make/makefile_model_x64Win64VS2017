CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".;$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export PATH := $(NDDSHOME)/lib/x64Win64VS2017;$(PATH)

TemperatureForecastPublisher: ./MessageTypes/Weather/TemperatureForecastPublisher.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" MessageTypes.Weather.TemperatureForecastPublisher $(ARGS)
 
TemperatureForecastSubscriber: ./MessageTypes/Weather/TemperatureForecastSubscriber.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" MessageTypes.Weather.TemperatureForecastSubscriber $(ARGS)
