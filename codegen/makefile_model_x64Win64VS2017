SOURCE_DIR = 

TARGET_ARCH=x64Win64VS2017

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := C:\Program Files\rti_connext_dds-5.3.1\bin\..
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./TemplateModule/StructA.java $(SOURCE_DIR)./TemplateModule/StructASeq.java $(SOURCE_DIR)./TemplateModule/StructATypeSupport.java $(SOURCE_DIR)./TemplateModule/StructATypeCode.java $(SOURCE_DIR)./TemplateModule/StructADataReader.java $(SOURCE_DIR)./TemplateModule/StructADataWriter.java $(SOURCE_DIR)./TemplateModule/StructB.java $(SOURCE_DIR)./TemplateModule/StructBSeq.java $(SOURCE_DIR)./TemplateModule/StructBTypeSupport.java $(SOURCE_DIR)./TemplateModule/StructBTypeCode.java $(SOURCE_DIR)./TemplateModule/StructBDataReader.java $(SOURCE_DIR)./TemplateModule/StructBDataWriter.java $(SOURCE_DIR)./TemplateModule/StructBSubscriber.java $(SOURCE_DIR)./TemplateModule/StructBPublisher.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".;$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export PATH := $(NDDSHOME)/lib/x64Win64VS2017;$(PATH)

StructBPublisher: ./TemplateModule/StructBPublisher.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" TemplateModule.StructBPublisher $(ARGS)
 
StructBSubscriber: ./TemplateModule/StructBSubscriber.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" TemplateModule.StructBSubscriber $(ARGS)
