SOURCE_DIR = 

TARGET_ARCH=x64Win64VS2017

# If undefined in the environment default NDDSHOME to install dir
ifndef NDDSHOME
NDDSHOME := C:\Program Files\rti_connext_dds-5.3.1\bin\..
endif

JAVA_PATH = java
JAVAC_PATH = javac

JAVA_SOURCES = $(SOURCE_DIR)./DataModel/LocationType.java $(SOURCE_DIR)./DataModel/LocationTypeSeq.java $(SOURCE_DIR)./DataModel/LocationTypeTypeSupport.java $(SOURCE_DIR)./DataModel/LocationTypeTypeCode.java $(SOURCE_DIR)./DataModel/LocationTypeDataReader.java $(SOURCE_DIR)./DataModel/LocationTypeDataWriter.java $(SOURCE_DIR)./DataModel/SensorMetaType.java $(SOURCE_DIR)./DataModel/SensorMetaTypeSeq.java $(SOURCE_DIR)./DataModel/SensorMetaTypeTypeSupport.java $(SOURCE_DIR)./DataModel/SensorMetaTypeTypeCode.java $(SOURCE_DIR)./DataModel/SensorMetaTypeDataReader.java $(SOURCE_DIR)./DataModel/SensorMetaTypeDataWriter.java $(SOURCE_DIR)./DataModel/Data.java $(SOURCE_DIR)./DataModel/DataSeq.java $(SOURCE_DIR)./DataModel/DataTypeSupport.java $(SOURCE_DIR)./DataModel/DataTypeCode.java $(SOURCE_DIR)./DataModel/DataDataReader.java $(SOURCE_DIR)./DataModel/DataDataWriter.java $(SOURCE_DIR)./Weather/WeatherLocation.java $(SOURCE_DIR)./Weather/WeatherLocationSeq.java $(SOURCE_DIR)./Weather/WeatherLocationTypeSupport.java $(SOURCE_DIR)./Weather/WeatherLocationTypeCode.java $(SOURCE_DIR)./Weather/WeatherLocationDataReader.java $(SOURCE_DIR)./Weather/WeatherLocationDataWriter.java $(SOURCE_DIR)./Weather/Weather.java $(SOURCE_DIR)./Weather/WeatherSeq.java $(SOURCE_DIR)./Weather/WeatherTypeSupport.java $(SOURCE_DIR)./Weather/WeatherTypeCode.java $(SOURCE_DIR)./Weather/WeatherDataReader.java $(SOURCE_DIR)./Weather/WeatherDataWriter.java $(SOURCE_DIR)./Weather/Temperature.java $(SOURCE_DIR)./Weather/TemperatureSeq.java $(SOURCE_DIR)./Weather/TemperatureTypeSupport.java $(SOURCE_DIR)./Weather/TemperatureTypeCode.java $(SOURCE_DIR)./Weather/TemperatureDataReader.java $(SOURCE_DIR)./Weather/TemperatureDataWriter.java $(SOURCE_DIR)./Weather/TemperatureSubscriber.java $(SOURCE_DIR)./Weather/TemperaturePublisher.java
CLASS_FILES  = $(JAVA_SOURCES:%.java=%.class)

RTI_CLASSPATH := $(NDDSHOME)/lib/java/nddsjava.jar

%.class : %.java
	$(JAVAC_PATH)  -classpath ".;$(RTI_CLASSPATH)" $<

all: $(CLASS_FILES)
 
#
# Convenient way to run the java programs
#

export PATH := $(NDDSHOME)/lib/x64Win64VS2017;$(PATH)

TemperaturePublisher: ./Weather/TemperaturePublisher.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" Weather.TemperaturePublisher $(ARGS)
 
TemperatureSubscriber: ./Weather/TemperatureSubscriber.class
	$(JAVA_PATH)  -classpath ".;$(RTI_CLASSPATH)" Weather.TemperatureSubscriber $(ARGS)
